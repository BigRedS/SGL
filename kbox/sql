select SOFTWARE_LABEL_JT.SOFTWARE_ID, SOFTWARE_LABEL_JT.LABEL_ID, MACHINE_SOFTWARE_JT.MACHINE_ID, count(*)
from MACHINE_SOFTWARE_JT
join SOFTWARE_LABEL_JT
on SOFTWARE_LABEL_JT.SOFTWARE_ID = MACHINE_SOFTWARE_JT.SOFTWARE_ID
where SOFTWARE_LABEL_JT.LABEL_ID = '52'
group by SOFTWARE_LABEL_JT.SOFTWARE_ID


gives:

SOFTWARE_ID 	LABEL_ID 	MACHINE_ID 	count(*)
89 	52 	73 	5
105 	52 	26 	37
140 	52 	1 	140
148 	52 	2 	33
158 	52 	44 	1
161 	52 	3 	2
181 	52 	44 	3
185 	52 	3 	5
188 	52 	44 	1
190 	52 	3 	1
209 	52 	3 	5
214 	52 	44 	1
217 	52 	3 	5
232 	52 	3 	5
234 	52 	3 	2
238 	52 	3 	5
