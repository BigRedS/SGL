select SOFTWARE_LABEL_JT.SOFTWARE_ID, SOFTWARE_LABEL_JT.LABEL_ID, count(*)
from MACHINE_SOFTWARE_JT
join SOFTWARE_LABEL_JT
on SOFTWARE_LABEL_JT.SOFTWARE_ID = MACHINE_SOFTWARE_JT.SOFTWARE_ID
group by LABEL_ID

Gives:

SOFTWARE_ID 	LABEL_ID 	count(*)
181 		41 		72
91	 	46 		277
969 		47 		752
608 		48 		1119
1023 		52 		528

